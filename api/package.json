{
   "name": "ezchat-api",
   "version": "1.0.0",
   "license": "MIT",
   "scripts": {
      "dev": "nodemon ./src/main.ts",
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "lint": "yarn typescript && yarn eslint && yarn format",
      "lint:fix": "yarn typescript && yarn eslint:fix && yarn format:fix",
      "format": "prettier --check ./**/*.{js,jsx,ts,tsx,json}",
      "format:fix": "prettier --write ./**/*.{js,jsx,ts,tsx,json}",
      "eslint": "eslint -c .eslintrc.json src/**",
      "eslint:fix": "eslint -c .eslintrc.json src/** --fix",
      "typescript": "tsc --noEmit",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./src/ormconfig.ts"
   },
   "dependencies": {
      "connect-redis": "^6.0.0",
      "cors": "^2.8.5",
      "express": "^4.17.1",
      "express-async-handler": "^1.2.0",
      "express-rate-limit": "^5.5.1",
      "express-session": "^1.17.2",
      "express-winston": "^4.2.0",
      "http-status-codes": "^2.1.4",
      "ioredis": "^4.28.2",
      "pg": "^8.7.1",
      "ramda": "^0.27.1",
      "rate-limit-redis": "^2.1.0",
      "reflect-metadata": "^0.1.10",
      "socket.io": "^4.4.0",
      "typeorm": "^0.2.41",
      "uuid": "^8.3.2",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@types/connect-redis": "^0.0.18",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/express-rate-limit": "^5.1.3",
      "@types/express-session": "^1.17.4",
      "@types/http-codes": "^1.0.2",
      "@types/ioredis": "^4.28.3",
      "@types/node": "^16.11.12",
      "@types/ramda": "^0.27.60",
      "@types/rate-limit-redis": "^1.7.3",
      "@typescript-eslint/eslint-plugin": "^5.7.0",
      "@typescript-eslint/parser": "^5.7.0",
      "eslint": "^8.4.1",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-airbnb-base-typescript": "^1.0.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.25.3",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^5.2.0",
      "nodemon": "^2.0.15",
      "prettier": "^2.5.1",
      "ts-node": "^10.4.0",
      "typescript": "^4.5.3"
   }
}
